{
	"name": "FFI",
	"type": "parserhook",
	"version": "1.0.0",
	"author": [
		"Marijn van Wezel"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:FFI",
	"descriptionmsg": "ffi-desc",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"platform": {
			"php": ">=7.3"
		}
	},
	"MessagesDirs": {
		"FFI": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"FFIMagic": "i18n/FFI.i18n.php"
	},
	"config": {
		"FFIEngines": {
			"value": {
				"py": {
					"class": "MediaWiki\\Extension\\FFI\\Engines\\Python\\PythonEngine"
				}
			}
		}
	},
	"namespaces": [
		{
			"id": 7670,
			"constant": "NS_SCRIPT",
			"name": "Script",
			"protection": "ffi-edit-script",
			"defaultcontentmodel": "script"
		},
		{
			"id": 7671,
			"constant": "NS_SCRIPT_TALK",
			"name": "Script_talk"
		}
	],
	"GroupPermissions": {
		"user": {
			"ffi-edit-script": true
		}
	},
	"ContentHandlers": {
		"script": "MediaWiki\\Extension\\FFI\\MediaWiki\\ContentHandlers\\ScriptContentHandler"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\FFI\\": "includes/"
	},
	"Hooks": {
		"EditFilterMergedContent": "main",
		"EditPageBeforeEditButtons": "main",
		"CodeEditorGetPageLanguage": "MediaWiki\\Extension\\FFI\\MediaWiki\\Hooks::onCodeEditorGetPageLanguage"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\FFI\\MediaWiki\\Hooks",
			"services": [
				"FFI.EngineFactory"
			]
		}
	},
	"ServiceWiringFiles": [
		"includes/FFI.ServiceWiring.php"
	],
	"ResourceModules": {
		"ext.ffi.ui": {
			"styles": "ext.ffi.ui.css"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "FFI/modules"
	},
	"callback": "MediaWiki\\Extension\\FFI\\MediaWiki\\Hooks::onRegistration",
	"manifest_version": 2
}
